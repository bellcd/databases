{
  "name": "chatterbox-client-and-database",
  "version": "1.1.0",
  "description": "A component-based chat-client using jQuery, backed with a MYSQL database",
  "author": "Hack Reactor <curriculum.team@hackreactor.com>",
  "license": "UNLICENSED",
  "private": true,
  "engines": {
    "node": ">=4.0.0"
  },
  "scripts": {
    "postinstall": "curl -s https://raw.githubusercontent.com/reactorcore/pomander/master/bin/install | bash",
    "start": "nodemon ./server/app.js",
    "start:es6": "babel-node ./server/app.js",
    "create-db": "mysql -u root < ./server/schema.sql",
    "test": "npm run create-db; npm --orm=no-orm run test-both",
    "test-orm": "npm --orm=orm run test-both",
    "test-orm-no-orm": "npm run create-db; npm run test; npm run test-orm",
    "test-both": "mocha server/spec/server-spec.js; RES=$?; kill %1; open ./SpecRunner.html; exit $RES;",
    "test-server": "mocha server/spec/server-spec.js; RES=$?; kill %1; exit $RES",
    "test-client": "open ./SpecRunner.html",
    "test:es6": "mocha --compilers js:babel-register server/spec/server-spec.js; RES=$?; kill %1; exit $RES",
    "lint": "eslint ./"
  },
  "dependencies": {
    "body-parser": "^1.9.2",
    "cors": "^2.8.5",
    "express": "^4.10.1",
    "jquery": "^3.3.1",
    "morgan": "^1.4.1",
    "mysql": "^2.17.1",
    "mysql2": "^1.7.0",
    "nodemon": "^1.19.3",
    "request": "^2.88.0",
    "sequelize": "^5.0.0",
    "underscore": "^1.9.1"
  },
  "devDependencies": {
    "babel-cli": "^6.7.5",
    "babel-preset-es2015": "^6.6.0",
    "babel-register": "^6.7.2",
    "sinon": "^6.3.4",
    "chai": "^4.1.2",
    "eslint-config-hackreactor": "git://github.com/reactorcore/eslint-config-hackreactor",
    "mocha": "^5.2.0"
  },
  "babel": {
    "presets": [
      "es2015"
    ]
  }
}
